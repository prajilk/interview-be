generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model admin {
  id         Int         @id @default(autoincrement())
  username   String      @db.VarChar(99) @unique
  password   String
}

model department {
  id            Int         @id @default(autoincrement())
  name          String      
  image         String      
  description   String
}